      xmlns:ctool="clr-namespace:Kirin_2.UserControls"
      mc:Ignorable="d" 
      d:DesignHeight="450"
      FontFamily="{DynamicResource MaterialDesignFont}"
      Width="{Binding ActualWidth, ElementName=parentElementName}"
      Title="Dashboard"
      xmlns:syncfusion1="http://schemas.syncfusion.com/wpf"
      xmlns:chart="clr-namespace:Syncfusion.UI.Xaml.Charts;assembly=Syncfusion.SfChart.WPF"
      xmlns:syncfusion="clr-namespace:Syncfusion.UI.Xaml.Maps;assembly=Syncfusion.SfMaps.WPF"
      xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
      Loaded="Page_Loaded"
      SizeChanged="Window_SizeChanged">

