<Page x:Class="Kirin_2.Pages.Datasandbox"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Kirin_2.Pages"
      xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
      xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
      xmlns:chart="clr-namespace:Syncfusion.UI.Xaml.Charts;assembly=Syncfusion.SfChart.WPF"
      mc:Ignorable="d" 
      FontFamily="{DynamicResource MaterialDesignFont}"
      xmlns:settings="clr-namespace:Kirin_2.Properties"
      d:DesignHeight="450" d:DesignWidth="800"
      Width="{Binding ActualWidth, ElementName=parentElementName}"
      xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
      Title="Datasandbox">
    <Page.Resources>
        <Style x:Key="FBButtonStyle" TargetType="Button">
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="Margin" Value="0 30 0 20" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Grid Background="{TemplateBinding Background}">
                            <ContentPresenter />
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Style.Triggers>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter Property="Background" Value="AliceBlue"/>
                </Trigger>
            </Style.Triggers>
        </Style>
        <Style TargetType="{x:Type Button}">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ClipToBounds="False">
                            <Grid.RowDefinitions>
                                <RowDefinition />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <!-- the background for the button -->
                            <Rectangle RadiusX="5" RadiusY="5" Grid.RowSpan="2" Height="30">
                                <Rectangle.Fill>
                                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" >
                                        <LinearGradientBrush.GradientStops>
                                            <GradientStop Color="#3d78a6" Offset="0"/>
                                            <GradientStop Color="#3d78a6" Offset="0.9"/>
                                        </LinearGradientBrush.GradientStops>
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>

                            <!-- place for the content inside the button to be displayed -->
                            <ContentPresenter Grid.RowSpan="2"  
              x:Name="PrimaryContent"
              HorizontalAlignment="Center" VerticalAlignment="Center"
              Margin="{TemplateBinding Padding}">
                                <ContentPresenter.ContentTemplate>
                                    <DataTemplate>
                                        <TextBlock Padding="5" Text="{Binding Path=Content, RelativeSource={RelativeSource TemplatedParent}}" FontSize="12" Foreground="WhiteSmoke" FontWeight="Bold"/>
                                    </DataTemplate>
                                </ContentPresenter.ContentTemplate>
                            </ContentPresenter>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="Foreground" Value="White" />
            <Setter Property="FontFamily" Value="Roboto" />
        </Style>

        <Style TargetType="Polyline" x:Key="CurveColorStyle">
            <Setter Property="Stroke" Value="LightSeaGreen"/>
            <Setter Property="StrokeThickness" Value="3"/>
            <Setter Property="StrokeDashArray" Value="1,2" />
        </Style>

        <Style x:Key="ExpandCollapseToggleStyle" TargetType="Expander">
            <Setter Property="Focusable" Value="False"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Expander">
                        <Grid Width="15" Height="13" Background="Transparent" >
                            <materialDesign:PackIcon Kind="ChevronDown" x:Name="ExpandPath" HorizontalAlignment="Right" 
                            VerticalAlignment="Center" Margin="1,1,1,1" />
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsExpanded" Value="True">
                                <Setter Property="Kind" TargetName="ExpandPath" Value="ChevronDown"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Page.Resources>
    <Grid>
        <Grid.ColumnDefinitions>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="2"/>
        </Grid.RowDefinitions>

        <ScrollViewer VerticalScrollBarVisibility="Auto" Grid.Row="0" Grid.Column="0">
            <StackPanel>
                <TextBlock Grid.Row="0" Grid.Column="0" FontSize="20" FontWeight="SemiBold" Margin="20 10 20 20">SANDBOX (COMPARATIVE ANALYSIS)</TextBlock>

                <StackPanel CanVerticallyScroll="True">
                    <TextBlock FontSize="16" FontWeight="Bold" TextAlignment="Left" Text="&lt; TITLE FOR ROW 1 GRAPH &gt;" Margin="10 10 0 5" />
                    <Border BorderThickness="1" BorderBrush="{Binding Source={x:Static settings:Settings.Default}, Path=Dashboard_Box_Outline, Mode=TwoWay }" 
                            Background="White" CornerRadius="5" Width="1130" Margin="10" Height="460">
                        <StackPanel>
                            <Grid>
                                <Grid Grid.Row="0">
                                    <ComboBox x:Name="cmbStudent" Margin="36 20 20 0" Width="150" HorizontalAlignment="Left" 
                                                      VerticalAlignment="Top" DisplayMemberPath="NAME" SelectedValuePath="ID" 
                                                      SelectedValue="{Binding ID}"
                                                      SelectionChanged="StudentOption_SelectionChanged"
                                                      FontWeight="Medium" FontSize="14"
                                                      BorderThickness="0" BorderBrush="White">
                                        <ComboBox.Resources>
                                            <Style TargetType="ComboBox">
                                                <Setter Property="Background" Value="White"/>
                                                <Setter Property="UIElement.SnapsToDevicePixels" Value="True"/>
                                                <Setter Property="TextElement.Foreground" Value="Black"/>
                                                <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto"/>
                                                <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
                                                <Setter Property="ScrollViewer.CanContentScroll" Value="True"/>
                                                <Setter Property="FrameworkElement.FocusVisualStyle" Value="{x:Null}"/>
                                                <Setter Property="ItemsPanel">
                                                    <Setter.Value>
                                                        <ItemsPanelTemplate>
                                                            <UniformGrid Height="180" Columns="3" Background="{Binding Source={x:Static settings:Settings.Default}, Path=Border_color_inner, Mode=TwoWay }" 
                                                                                 ScrollViewer.VerticalScrollBarVisibility="Auto"
                                                                                 ScrollViewer.HorizontalScrollBarVisibility="Visible" >
                                                                <UniformGrid.Resources>
                                                                    <Style TargetType="UniformGrid">
                                                                        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Visible"/>
                                                                    </Style>
                                                                    <Style TargetType="TextBlock">
                                                                        <Setter Property="Foreground" Value="White"/>
                                                                    </Style>
                                                                </UniformGrid.Resources>
                                                            </UniformGrid>
                                                        </ItemsPanelTemplate>
                                                    </Setter.Value>
                                                </Setter>
                                            </Style>
                                            <Style TargetType="ComboBoxItem">
                                                <Setter Property="Width" Value="105" />
                                                <Setter Property="TextElement.Foreground" Value="White"/>
                                                <Setter Property="ToolTip" Value="{Binding NAME}"/>
                                            </Style>
                                        </ComboBox.Resources>
                                    </ComboBox>
                                </Grid>
                                <Grid Grid.Row="1">
                                    <!--<Button Name="btnDBackward" Click="DoubleBackButton_Click" Style="{StaticResource FBButtonStyle}" BorderThickness="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="30 0 0 0">
                                        <materialDesign:PackIcon Kind="ChevronDoubleLeft" Foreground="#247BC0" Width="25" Height="25"/>
                                    </Button>-->
                                    <Button Click="BackButton_Click" Style="{StaticResource FBButtonStyle}" BorderThickness="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="30 0 0 0">
                                        <materialDesign:PackIcon Kind="ArrowLeft" Foreground="#247BC0" Width="25" Height="25"/>
                                    </Button>
                                    <StackPanel Orientation="Horizontal">
                                        <chart:SfChart x:Name="RadarChartSubject" Visibility="Visible" Background="White" Height="400" Margin="60 30 20 20" Width="550" >
                                            <!--SelectionChanged="RadarChartSubject_SelectionChanged"-->
                                            <chart:SfChart.PrimaryAxis>
                                                <chart:CategoryAxis IsHitTestVisible="False" TickLineSize="15" FontSize="14" FontWeight="Bold" />
                                            </chart:SfChart.PrimaryAxis>

                                            <chart:SfChart.SecondaryAxis>
                                                <chart:NumericalAxis EdgeLabelsDrawingMode="Fit" SmallTickLineSize="6" TickLineSize="10" Minimum="10" FontSize="14" FontWeight="Bold" />
                                            </chart:SfChart.SecondaryAxis>

                                            <chart:RadarSeries x:Name="SubjectSeries" ItemsSource="{Binding subjectDetails}" Interior="#068dff" Stroke="Black"
                                           StrokeThickness="2" XBindingPath="SubjectName" YBindingPath="Score" ShowTooltip="True" Opacity="0.8">
                                            </chart:RadarSeries>

                                            <chart:SfChart.Behaviors>
                                                <chart:ChartSelectionBehavior SelectionCursor="Hand" EnableSegmentSelection="True" />
                                            </chart:SfChart.Behaviors>
                                        </chart:SfChart>

                                        <DataGrid Width="380" x:Name="DGSubjects" Height="340" Background="Transparent" HeadersVisibility="Column"
                                              Focusable="False" RowDetailsVisibilityMode="Visible"
                                              RowDetailsVisibilityChanged="DGSubjects_RowDetailsVisibilityChanged" 
                                              ItemsSource="{Binding StdSubjectView}" AutoGenerateColumns="False" Margin="10,80,25,10"
                                              CanUserAddRows="False" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                                            <DataGrid.CellStyle>
                                                <Style TargetType="DataGridCell">
                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                    <Setter Property="Focusable" Value="False"/>
                                                    <Setter Property="Height" Value="40" />
                                                    <Setter Property="Foreground" Value="#034d6d" />
                                                </Style>
                                            </DataGrid.CellStyle>
                                            <DataGrid.Columns>
                                                <DataGridTemplateColumn Header="SUBJECT">
                                                    <DataGridTemplateColumn.HeaderStyle>
                                                        <Style TargetType="DataGridColumnHeader">
                                                            <Setter Property="Height" Value="30" />
                                                            <Setter Property="Width" Value="230" />
                                                            <Setter Property="FontSize" Value="16" />
                                                            <Setter Property="FontWeight" Value="DemiBold" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Left"/>
                                                            <Setter Property="Background" Value="Transparent"/>
                                                        </Style>
                                                    </DataGridTemplateColumn.HeaderStyle>
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                                                <TextBlock Text="{Binding Path=SubjectName}" FontSize="16" FontWeight="Medium" TextAlignment="Left" HorizontalAlignment="Left"></TextBlock>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                                <DataGridTemplateColumn Header="GRADE(%)">
                                                    <DataGridTemplateColumn.HeaderStyle>
                                                        <Style TargetType="DataGridColumnHeader">
                                                            <Setter Property="Height" Value="30" />
                                                            <Setter Property="Width" Value="110" />
                                                            <Setter Property="FontSize" Value="16" />
                                                            <Setter Property="FontWeight" Value="DemiBold" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Right"/>
                                                            <Setter Property="Background" Value="Transparent"/>
                                                        </Style>
                                                    </DataGridTemplateColumn.HeaderStyle>
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel VerticalAlignment="Center">
                                                                <TextBlock TextAlignment="Right" Text="{Binding Path=Score, UpdateSourceTrigger=PropertyChanged}" FontSize="16"></TextBlock>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                            </DataGrid.Columns>
                                            <DataGrid.RowDetailsTemplate>
                                                <DataTemplate>
                                                    <DataGrid x:Name="DGChildView" HeadersVisibility="Column" IsReadOnly="True" Width="Auto" Background="Transparent"
                                                        AutoGenerateColumns="False" Focusable="False" Height="Auto" CanUserAddRows="False" VerticalScrollBarVisibility="Auto"
                                                        RowDetailsVisibilityChanged="DGChildView_RowDetailsVisibilityChanged" ItemsSource="{Binding childView}" >
                                                        <DataGrid.CellStyle>
                                                            <Style TargetType="DataGridCell">
                                                                <Setter Property="Height" Value="30"/>
                                                                <Setter Property="VerticalAlignment" Value="Center" />
                                                                <Setter Property="Focusable" Value="False"/>
                                                            </Style>
                                                        </DataGrid.CellStyle>
                                                        <DataGrid.Columns>
                                                            <DataGridTemplateColumn>
                                                                <DataGridTemplateColumn.HeaderStyle>
                                                                    <Style TargetType="DataGridColumnHeader">
                                                                        <Setter Property="Height" Value="30" />
                                                                        <Setter Property="Width" Value="180" />
                                                                        <Setter Property="FontSize" Value="16" />
                                                                        <Setter Property="FontWeight" Value="DemiBold" />
                                                                        <Setter Property="HorizontalContentAlignment" Value="Left"/>
                                                                        <Setter Property="Background" Value="Transparent"/>
                                                                    </Style>
                                                                </DataGridTemplateColumn.HeaderStyle>
                                                                <DataGridTemplateColumn.CellTemplate>
                                                                    <DataTemplate>
                                                                        <StackPanel Orientation="Horizontal" Focusable="False" VerticalAlignment="Center" Width="Auto">
                                                                            <Expander Width="30" HorizontalAlignment="Left" HorizontalContentAlignment="Stretch" 
                                                                                  Expanded="ChildExpander_Expanded" Collapsed="ChildExpander_Collapsed"
                                                                                  Style="{StaticResource ExpandCollapseToggleStyle}" 
                                                                                  Focusable="False">
                                                                            </Expander>
                                                                            <TextBlock Text="{Binding Path=label}" FontSize="16" FontWeight="Bold" Focusable="False"></TextBlock>
                                                                        </StackPanel>
                                                                    </DataTemplate>
                                                                </DataGridTemplateColumn.CellTemplate>
                                                            </DataGridTemplateColumn>
                                                        </DataGrid.Columns>
                                                        <DataGrid.RowDetailsTemplate>
                                                            <DataTemplate>
                                                                <DataGrid x:Name="DGSubChildView" HeadersVisibility="Column" IsReadOnly="True" Width="Auto" Background="Transparent"
                                                                    AutoGenerateColumns="False" Focusable="False" Height="Auto" CanUserAddRows="False" VerticalScrollBarVisibility="Auto"
                                                                    ItemsSource="{Binding subChildView}">
                                                                    <DataGrid.CellStyle>
                                                                        <Style TargetType="DataGridCell">
                                                                            <Setter Property="Height" Value="30"/>
                                                                            <Setter Property="VerticalAlignment" Value="Center" />
                                                                            <Setter Property="Focusable" Value="False"/>
                                                                        </Style>
                                                                    </DataGrid.CellStyle>
                                                                    <DataGrid.Columns>
                                                                        <DataGridTextColumn Width="170" Binding="{Binding Path=Assignment, Mode=TwoWay}" Visibility="Visible" IsReadOnly="True" 
                                                                            Header="ASSIGNMENT" FontSize="16" FontWeight="Medium">
                                                                            <DataGridTextColumn.HeaderStyle>
                                                                                <Style TargetType="DataGridColumnHeader">
                                                                                    <Setter Property="Height" Value="30" />
                                                                                    <Setter Property="Width" Value="170" />
                                                                                    <Setter Property="FontSize" Value="16" />
                                                                                    <Setter Property="FontWeight" Value="DemiBold" />
                                                                                    <Setter Property="HorizontalContentAlignment" Value="Left"/>
                                                                                    <Setter Property="Background" Value="Transparent"/>
                                                                                </Style>
                                                                            </DataGridTextColumn.HeaderStyle>
                                                                            <DataGridTextColumn.ElementStyle>
                                                                                <Style TargetType="{x:Type TextBlock}">
                                                                                    <Setter Property="HorizontalAlignment" Value="Left" />
                                                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                                                    <Setter Property="TextAlignment" Value="Center" />
                                                                                </Style>
                                                                            </DataGridTextColumn.ElementStyle>
                                                                        </DataGridTextColumn>
                                                                        <DataGridTextColumn Width="80" Binding="{Binding Path=PointsPossible, Mode=TwoWay}" Visibility="Visible" IsReadOnly="True" 
                                                                            Header="POINTS" FontSize="16" FontWeight="Medium">
                                                                            <DataGridTextColumn.HeaderStyle>
                                                                                <Style TargetType="DataGridColumnHeader">
                                                                                    <Setter Property="Height" Value="30" />
                                                                                    <Setter Property="Width" Value="80" />
                                                                                    <Setter Property="FontSize" Value="16" />
                                                                                    <Setter Property="FontWeight" Value="DemiBold" />
                                                                                    <Setter Property="HorizontalContentAlignment" Value="Right"/>
                                                                                    <Setter Property="VerticalContentAlignment" Value="Center"/>
                                                                                    <Setter Property="Background" Value="Transparent"/>
                                                                                </Style>
                                                                            </DataGridTextColumn.HeaderStyle>
                                                                            <DataGridTextColumn.ElementStyle>
                                                                                <Style TargetType="{x:Type TextBlock}">
                                                                                    <Setter Property="HorizontalAlignment" Value="Right" />
                                                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                                                    <Setter Property="Focusable" Value="False" />
                                                                                </Style>
                                                                            </DataGridTextColumn.ElementStyle>
                                                                        </DataGridTextColumn>
                                                                        <DataGridTextColumn Width="80" Binding="{Binding Path=PointsEarned, Mode=TwoWay}" Visibility="Visible" IsReadOnly="True" 
                                                                            Header="EARNED" FontSize="16" FontWeight="Medium">
                                                                            <DataGridTextColumn.HeaderStyle>
                                                                                <Style TargetType="DataGridColumnHeader">
                                                                                    <Setter Property="Height" Value="30" />
                                                                                    <Setter Property="Width" Value="80" />
                                                                                    <Setter Property="FontSize" Value="16" />
                                                                                    <Setter Property="FontWeight" Value="DemiBold" />
                                                                                    <Setter Property="HorizontalContentAlignment" Value="Right"/>
                                                                                    <Setter Property="VerticalContentAlignment" Value="Center"/>
                                                                                    <Setter Property="Background" Value="Transparent"/>
                                                                                </Style>
                                                                            </DataGridTextColumn.HeaderStyle>
                                                                            <DataGridTextColumn.ElementStyle>
                                                                                <Style TargetType="{x:Type TextBlock}">
                                                                                    <Setter Property="HorizontalAlignment" Value="Right" />
                                                                                    <Setter Property="VerticalAlignment" Value="Center" />
                                                                                    <Setter Property="Focusable" Value="False" />
                                                                                </Style>
                                                                            </DataGridTextColumn.ElementStyle>
                                                                        </DataGridTextColumn>
                                                                        <DataGridTemplateColumn>
                                                                            <DataGridTemplateColumn.CellTemplate>
                                                                                <DataTemplate>
                                                                                    <materialDesign:PackIcon Kind="{Binding Path=ArrowType, Mode=TwoWay}" Grid.Row="1" HorizontalAlignment="Right" VerticalAlignment="Center" Foreground="{Binding Path=GrowthColor, Mode=TwoWay}"></materialDesign:PackIcon>
                                                                                </DataTemplate>
                                                                            </DataGridTemplateColumn.CellTemplate>
                                                                        </DataGridTemplateColumn>
                                                                    </DataGrid.Columns>
                                                                </DataGrid>
                                                            </DataTemplate>
                                                        </DataGrid.RowDetailsTemplate>
                                                    </DataGrid>
                                                </DataTemplate>
                                            </DataGrid.RowDetailsTemplate>
                                        </DataGrid>
                                    </StackPanel>
                                    <Button Click="ForwardButton_Click" Style="{StaticResource FBButtonStyle}" BorderThickness="0" HorizontalAlignment="Right" 
                                        VerticalAlignment="Center" Margin="40 0 70 0">
                                        <materialDesign:PackIcon Kind="ArrowRight" Foreground="#247BC0" Width="25" Height="25"/>
                                    </Button>
                                    <!--<Button Name="btnDForward" Click="DoubleForwardButton_Click" Style="{StaticResource FBButtonStyle}" BorderThickness="0" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="10 0 60 0">
                                        <materialDesign:PackIcon Kind="ChevronDoubleRight" Foreground="#247BC0" Width="25" Height="25"/>
                                    </Button>-->
                                </Grid>
                            </Grid>
                        </StackPanel>
                    </Border>

                    <TextBlock FontSize="16" FontWeight="Bold" TextAlignment="Left" Text="&lt; TITLE FOR ROW 2 GRAPH &gt;" Margin="10 20 0 5" />
                    <Border BorderThickness="1" BorderBrush="{Binding Source={x:Static settings:Settings.Default}, Path=Dashboard_Box_Outline, Mode=TwoWay }" 
                            Background="White" CornerRadius="5" Width="1130" Margin="10" Height="460">
                        <StackPanel Margin="1" Orientation="Vertical">
                            <Grid>
                                <Grid Grid.Row="0" HorizontalAlignment="Left">
                                    <StackPanel Orientation="Horizontal" Margin="10 10 0 20" HorizontalAlignment="Left" VerticalAlignment="Top">

                                        <Label Content="Grades " FontSize="17" FontWeight="DemiBold" Background="Transparent" Foreground="Black" IsHitTestVisible="False"
                                                       HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10 15 0 0" HorizontalContentAlignment="Left"></Label>
                                        <Label Width="50" Content="V/S" FontWeight="Bold" FontSize="16" VerticalAlignment="Top" Margin="5 15 0 0"></Label>
                                        <ComboBox x:Name="ParameterOption2" Style="{StaticResource MaterialDesignFilledComboBox}"
                                                  Width="160" HorizontalAlignment="Left" VerticalAlignment="Top"
                                                    SelectionChanged="Param2_SelectionChanged" FontWeight="DemiBold" FontSize="16" Background="Transparent">
                                        </ComboBox>
                                        <ComboBox x:Name="GraphOption" Margin="20 0" Style="{DynamicResource MaterialDesignFilledComboBox}" Visibility="Hidden"
                                                  SelectionChanged="GraphOption_SelectionChanged" FontWeight="DemiBold" FontSize="16" Background="Transparent" Height="40"
                                                  Width="160" HorizontalAlignment="Left" VerticalAlignment="Top">
                                        </ComboBox>
                                        <ComboBox x:Name="ParameterOption1" Style="{StaticResource MaterialDesignFilledComboBox}"
                                          SelectionChanged="Param1_SelectionChanged" FontWeight="DemiBold" FontSize="16" Background="Transparent"
										  HorizontalAlignment="Left" VerticalAlignment="Top" IsReadOnly="True" IsHitTestVisible="False" Visibility="Hidden">
                                        </ComboBox>
                                    </StackPanel>
                                </Grid>
                                <Grid Grid.Row="1">
                                    <Button Click="btnDBackwardRow2_Click" Style="{StaticResource FBButtonStyle}" BorderThickness="0" 
                                    HorizontalAlignment="Left" VerticalAlignment="Center" Margin="20 0 0 0">
                                        <materialDesign:PackIcon Kind="ChevronDoubleLeft" Foreground="#247BC0" Width="25" Height="25"/>
                                    </Button>
                                    <Button Click="BackButtonRow2_Click" Style="{StaticResource FBButtonStyle}" BorderThickness="0" 
                                        HorizontalAlignment="Left" VerticalAlignment="Center" Margin="60 0 0 0">
                                        <materialDesign:PackIcon Kind="ArrowLeft" Foreground="#247BC0" Width="25" Height="25"/>
                                    </Button>
                                    <StackPanel Orientation="Horizontal">
                                        <chart:SfChart x:Name="diversityAreaChart" HorizontalAlignment="Left"
                                        Palette="Custom" Height="380" Margin="50 60 10 20" Width="600"
                                        Visibility="Collapsed" FontSize="14" FontWeight="Bold">
                                            <chart:SfChart.PrimaryAxis>
                                                <chart:CategoryAxis LabelPlacement="OnTicks" ShowGridLines="False" FontSize="14" FontWeight="Bold" PlotOffset="0" />
                                            </chart:SfChart.PrimaryAxis>

                                            <chart:SfChart.SecondaryAxis>
                                                <chart:NumericalAxis Maximum="100" Minimum="20" FontSize="14" FontWeight="Bold" />
                                            </chart:SfChart.SecondaryAxis>

                                            <chart:AreaSeries x:Name="AreaSeries" EnableAnimation="True" ShowTooltip="True" 
                                                    ItemsSource="{Binding DiversityAreaChartDetails}"
                                                    XBindingPath="Diversity" YBindingPath="Grade" Interior="#068dff"/>

                                            <chart:SfChart.Behaviors>
                                                <chart:ChartSelectionBehavior SelectionCursor="Hand" EnableSegmentSelection="True" />
                                            </chart:SfChart.Behaviors>
                                        </chart:SfChart>
                                        <DataGrid Width="340" x:Name="DGDiversityArea" Height="340" Background="Transparent" HeadersVisibility="Column" Focusable="False"
                                                      ItemsSource="{Binding DiversityAreaList}" AutoGenerateColumns="False" Margin="10,100,20,10" Visibility="Collapsed"
                                                      CanUserAddRows="False" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Auto">
                                            <DataGrid.CellStyle>
                                                <Style TargetType="DataGridCell">
                                                    <Setter Property="Height" Value="40"/>
                                                    <Setter Property="Focusable" Value="False"/>
                                                </Style>
                                            </DataGrid.CellStyle>
                                            <DataGrid.Columns>
                                                <DataGridTemplateColumn Header="DIVERSITY">
                                                    <DataGridTemplateColumn.HeaderStyle>
                                                        <Style TargetType="DataGridColumnHeader">
                                                            <Setter Property="Height" Value="30" />
                                                            <Setter Property="Width" Value="200" />
                                                            <Setter Property="FontSize" Value="16" />
                                                            <Setter Property="FontWeight" Value="DemiBold" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Left"/>
                                                            <Setter Property="VerticalContentAlignment" Value="Center"/>
                                                            <Setter Property="Background" Value="Transparent"/>
                                                        </Style>
                                                    </DataGridTemplateColumn.HeaderStyle>
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" Focusable="False">
                                                                <Grid>
                                                                    <TextBlock Text="{Binding Path=Diversity}" FontSize="16" FontWeight="Medium" Focusable="False"></TextBlock>
                                                                    <!--<ProgressBar Value="{Binding Path=Progress}" Maximum="10" Height="5" Foreground="#068dff"
                                                                                     Background="Transparent" BorderBrush="Transparent" Margin="0 15 0 0" />-->
                                                                </Grid>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                                <DataGridTemplateColumn Header="GRADE(%)">
                                                    <DataGridTemplateColumn.HeaderStyle>
                                                        <Style TargetType="DataGridColumnHeader">
                                                            <Setter Property="Height" Value="30" />
                                                            <Setter Property="Width" Value="110" />
                                                            <Setter Property="FontSize" Value="16" />
                                                            <Setter Property="FontWeight" Value="DemiBold" />
                                                            <Setter Property="HorizontalContentAlignment" Value="Right"/>
                                                            <Setter Property="VerticalContentAlignment" Value="Center"/>
                                                            <Setter Property="Background" Value="Transparent"/>
                                                        </Style>
                                                    </DataGridTemplateColumn.HeaderStyle>
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel VerticalAlignment="Center" Focusable="False">
                                                                <TextBlock TextAlignment="Right" Text="{Binding Path=Grade, UpdateSourceTrigger=PropertyChanged}" FontSize="16" Focusable="False"></TextBlock>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                            </DataGrid.Columns>
                                        </DataGrid>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal">
                                        <chart:SfChart x:Name="diversityBubble" Height="380" Margin="40 60 10 20" Width="600"
                                                   HorizontalAlignment="Left" Visibility="Collapsed" FontSize="14" FontWeight="Bold">
                                            <chart:SfChart.PrimaryAxis>
                                                <chart:CategoryAxis ShowGridLines="False" PlotOffset="35" ShowTrackBallInfo="False" 
                                                                LabelPlacement="OnTicks" FontSize="14" FontWeight="Bold" />
