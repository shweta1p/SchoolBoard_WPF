<Page x:Class="Kirin_2.Pages.ClassMultiDayAttendance"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Kirin_2.Pages"
      xmlns:settings="clr-namespace:Kirin_2.Properties"
      xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
      FontFamily="{DynamicResource MaterialDesignFont}"
      mc:Ignorable="d"  
      d:DesignHeight="450" d:DesignWidth="1000"
      Width="Auto"
      Title="ClassMultiDayAttendance">

    <Page.Resources>
        <ResourceDictionary>
            <Style TargetType="{x:Type DataGridCell}">
                <Setter Property="Focusable" Value="False"/>
                <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
            </Style>

            <ControlTemplate x:Key="scroll_background" TargetType="RepeatButton">
                <Border Background="Transparent">
                </Border>
            </ControlTemplate>

            <Style x:Key="scroll_thumb_style" TargetType="Thumb">
                <Setter Property="SnapsToDevicePixels" Value="True"/>
                <Setter Property="OverridesDefaultStyle" Value="true"/>
                <Setter Property="IsTabStop" Value="false"/>
                <Setter Property="Focusable" Value="false"/>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="Thumb">
                            <Rectangle Width="10" Fill="#7D7D7D" RadiusX="5" RadiusY="5">
                            </Rectangle>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style x:Key="scroll_thumb_stylecrosswise" TargetType="Thumb">
                <Setter Property="SnapsToDevicePixels" Value="True"/>
                <Setter Property="OverridesDefaultStyle" Value="true"/>
                <Setter Property="IsTabStop" Value="false"/>
                <Setter Property="Focusable" Value="false"/>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="Thumb">
                            <Rectangle  Fill="#7D7D7D" RadiusX="5" RadiusY="5">
                            </Rectangle>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <DataTemplate x:Key="HeaderTemplate_TOTAL" >
                <Grid
            VerticalAlignment="Top" Margin="0,0,0 ,-20" Width="86">
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Label
            VerticalContentAlignment="Top" Grid.Row="0" Padding="0" HorizontalAlignment="Center" FontSize="14" FontWeight="Bold" Foreground="White" Grid.ColumnSpan="2" Grid.Column="0" Content="Total"/>
                    <Label
            Grid.Row="1" Padding="0 0 4 0" HorizontalContentAlignment="Center" FontSize="11" FontWeight="Bold" Foreground="White" Content="Absent" Grid.Column="0"/>
                    <Label
            Grid.Row="1" Padding="0 0 4 0" FontSize="11" FontWeight="Bold" Foreground="White" Content="Late" Grid.Column="1"/>
                </Grid>
            </DataTemplate>

            <DataTemplate x:Key="HeaderTemplate_STUDENTS" >
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />

                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>

                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Label Grid.Row="0" Grid.Column="0" VerticalAlignment="Top" FontWeight="Bold" FontSize="18" Margin="-15,13,-2.2,-22" Foreground="White" >Students</Label>
                </Grid>
            </DataTemplate>

            <DataTemplate x:Key="HeaderTemplate_Week1">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5" VerticalAlignment="Top" Margin="0,-22,0,0" Padding="0,0,0,0" FontSize="14" Foreground="White"  FontWeight="Bold"  Content="{Binding Path=DataContext.Week1DateRange, RelativeSource={RelativeSource AncestorType=DataGrid, Mode=FindAncestor}}" />
                    <Button Grid.Column="0" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">M</Button>
                    <Button Grid.Column="1" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">T</Button>
                    <Button Grid.Column="2" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">W</Button>
                    <Button Grid.Column="3" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">H</Button>
                    <Button Grid.Column="4" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">F</Button>
                </Grid>
            </DataTemplate>
            <DataTemplate x:Key="HeaderTemplate_Week2">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5" VerticalAlignment="Top"  Margin="0,-22,0,0" FontSize="14" FontWeight="Bold" Foreground="White"  Padding="0,0,0,0" Content="{Binding Path=DataContext.Week2DateRange, RelativeSource={RelativeSource AncestorType=DataGrid, Mode=FindAncestor}}" />
                    <Button Grid.Column="0" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">M</Button>
                    <Button Grid.Column="1" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">T</Button>
                    <Button Grid.Column="2" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">W</Button>
                    <Button Grid.Column="3" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">H</Button>
                    <Button Grid.Column="4" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">F</Button>
                </Grid>
            </DataTemplate>
            <DataTemplate x:Key="HeaderTemplate_Week3">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5" VerticalAlignment="Top"  Margin="0,-22,0,0" FontSize="14" FontWeight="Bold" Foreground="White"  Padding="0,0,0,0"  Content="{Binding Path=DataContext.Week3DateRange, RelativeSource={RelativeSource AncestorType=DataGrid, Mode=FindAncestor}}" />
                    <Button Grid.Column="0" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">M</Button>
                    <Button Grid.Column="1" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">T</Button>
                    <Button Grid.Column="2" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">W</Button>
                    <Button Grid.Column="3" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">H</Button>
                    <Button Grid.Column="4" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">F</Button>
                </Grid>
            </DataTemplate>
            <DataTemplate x:Key="HeaderTemplate_Week4">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5" VerticalAlignment="Top"  Margin="0,-22,0,0" FontSize="14" FontWeight="Bold" Foreground="White"  Padding="0,0,0,0"  Content="{Binding Path=DataContext.Week4DateRange, RelativeSource={RelativeSource AncestorType=DataGrid, Mode=FindAncestor}}" />
                    <Button Grid.Column="0" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">M</Button>
                    <Button Grid.Column="1" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">T</Button>
                    <Button Grid.Column="2" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">W</Button>
                    <Button Grid.Column="3" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">H</Button>
                    <Button Grid.Column="4" Grid.Row="1" Width="45" Margin="0,0,-0.2,-40">F</Button>
                </Grid>
            </DataTemplate>

            <Style TargetType="Button" x:Key="TabButton">
                <Setter Property="Background" Value="White" />
                <Setter Property="TextBlock.TextAlignment" Value="Center" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="Button">
                            <Border CornerRadius="0" Background="White" BorderBrush="#ccc" BorderThickness="0,1,1,0" >
                                <ContentPresenter x:Name="contentPresenter" ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style TargetType="Button" x:Key="TabButtonFirst">
                <Setter Property="Background" Value="White" />
                <Setter Property="TextBlock.TextAlignment" Value="Center" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="Button">
                            <Border CornerRadius="3,0,0,0" Background="White" BorderBrush="#ccc" BorderThickness="0,1,1,0" >
                                <ContentPresenter x:Name="contentPresenter" ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style TargetType="Button" x:Key="TabButtonLast">
                <Setter Property="Background" Value="White" />
                <Setter Property="TextBlock.TextAlignment" Value="Center" />
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="Button">
                            <Border CornerRadius="0,0,0,3" Background="White" BorderBrush="#ccc" BorderThickness="0,1,1,0" >
                                <ContentPresenter x:Name="contentPresenter" ContentTemplate="{TemplateBinding ContentTemplate}" Content="{TemplateBinding Content}" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" Margin="{TemplateBinding Padding}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}"/>
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>

            <Style TargetType="{x:Type Button}">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" ClipToBounds="False">
                                <Grid.RowDefinitions>
                                    <RowDefinition />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <!-- the background for the button -->
                                <Rectangle   RadiusX="5" RadiusY="5" Grid.RowSpan="2" Height="20">
                                    <Rectangle.Fill>
                                        <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" >
                                            <LinearGradientBrush.GradientStops>
                                                <GradientStop Color="#3d78a6" Offset="0"/>
                                                <GradientStop Color="#3d78a6" Offset="0.9"/>
                                            </LinearGradientBrush.GradientStops>
                                        </LinearGradientBrush>
                                    </Rectangle.Fill>
                                </Rectangle>

                                <!-- place for the content inside the button to be displayed -->
                                <ContentPresenter Grid.RowSpan="2"  
              x:Name="PrimaryContent"
              HorizontalAlignment="Center" VerticalAlignment="Center"
              Margin="{TemplateBinding Padding}"
              Content="{Binding Path=Content, RelativeSource={RelativeSource TemplatedParent}}" />
                            </Grid>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Setter Property="Foreground" Value="White" />
                <Setter Property="FontFamily" Value="Roboto" />
            </Style>
        </ResourceDictionary>
    </Page.Resources>
    <Grid>
        <StackPanel>
            <Viewbox Stretch="Uniform">
                <Grid>
                    <Grid Background="WhiteSmoke" >
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="*"/>
                            <RowDefinition Height="2*"/>
                            <RowDefinition Height="10*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <Label x:Name="ClassTitle" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" HorizontalAlignment="Left" 
                               VerticalAlignment="Top" FontSize="20" FontWeight="SemiBold" Margin="5"></Label>

                        <StackPanel Grid.Row="1" Grid.Column="0" Orientation="Horizontal" Margin="20 10 20 0">
                            <Button Width="90" FontSize="14" Content="Single Day" x:Name="TabButtonFirst" Click="SwitchToSingleDayView" Tag="{Binding ClassID}"  />
                            <Button Width="90" FontSize="14" Content="Multi-Day" x:Name="TabButtonLast"  />
                        </StackPanel>

                        <!--Group box for Attendance Codes-->
                        <Border BorderThickness="1" Grid.Row="2" Grid.Column="0" Margin="-25,20,15,10"
                           Panel.ZIndex="-100" Width="220" Height="70"
                           CornerRadius="5,5,5,5" BorderBrush="LightGray" Background="Transparent"/>
                        <Label Grid.Row="2" Grid.Column="0" Margin="45,45,0,5" FontWeight="DemiBold" FontSize="14">Attendance Code</Label>
                        <ComboBox Grid.Row="2" Grid.Column="0" ItemsSource="{Binding AttendanceCodes, Mode=OneWay, UpdateSourceTrigger=PropertyChanged}" 
                          SelectedItem="{Binding Path=AttendanceCodeSelectedItemMultiDayView, UpdateSourceTrigger=PropertyChanged}"
                          DisplayMemberPath="desc" Margin="45,30,0,0" Width="180" HorizontalAlignment="Left"  
                          Style="{StaticResource ComboBoxFlatStyle}" VerticalAlignment="Center" FontSize="14" />
                        <!--end :: Group box for Attendance Codes-->

                        <!--Group box for Date -->
                        <Border BorderThickness="1" Grid.Row="2" Grid.Column="1" Margin="-140,20,-60,10" Panel.ZIndex="-100" Width="280" Height="70" CornerRadius="5,5,5,5" BorderBrush="LightGray" Background="Transparent"/>
                        <Label Grid.Row="2" Grid.Column="1" Margin="10,45,0,0" FontWeight="DemiBold" FontSize="14">Date Range</Label>
                        <Label Width="190" Margin="10,70,0,10" Grid.Row="2" Content="{Binding Path=DateRangeFilter_lbl }" x:Name="DateRange" Grid.Column="1" HorizontalAlignment="Left" FontSize="14" />

                        <Button Width="30" Content="Edit" Grid.Row="2" Grid.Column="1" Margin="200,60,0,0" Height="46" VerticalAlignment="Top" HorizontalAlignment="Left" 
                                Command="{Binding EditDateRangeCommand}" CommandParameter="{Binding}" FontSize="14"/>
                        <!--end :: Group box for Date-->

                        <!--Group box for Class -->
                        <Border BorderThickness="1" Grid.Row="2" Grid.Column="2" Margin="-50,20,15,10" Panel.ZIndex="-100" Width="200" Height="70" CornerRadius="5,5,5,5" BorderBrush="LightGray" Background="Transparent"/>
                        <Label Grid.Row="2" Grid.Column="2" Margin="25,45,0,0" FontWeight="DemiBold" FontSize="14">Classes</Label>
                        <Button Grid.Row="2" Grid.Column="2" Width="150" Margin="-75,20,0,0" Content="Show Multiple Sections" FontSize="14"/>
                        <!--end :: Group box for Classes-->

                        <Border BorderThickness="0.5" BorderBrush="LightGray" VerticalAlignment="Bottom"
                            Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="4"/>

                        <StackPanel Grid.Row="2" Grid.Column="3" Orientation="Horizontal" >
                            <materialDesign:ColorZone
                            Margin="-20 20 10 0"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Left"
                            HorizontalContentAlignment="Left"
                            Mode="Light"
                            materialDesign:ShadowAssist.ShadowDepth="Depth1"
                            CornerRadius="20"
                            Height="30" Width="180" Background="WhiteSmoke" >
                                <StackPanel Width="180" VerticalAlignment="Center" HorizontalAlignment="Right" Orientation="Horizontal">
                                    <Button Style="{DynamicResource MaterialDesignToolButton}" VerticalContentAlignment="Center">
                                        <materialDesign:PackIcon Kind="Search" Width="15" Height="15" Opacity=".56"  />
                                    </Button>
                                    <TextBox Width="140" FontSize="14" FontWeight="Bold"
                                        Margin="-4 0 0 0" materialDesign:HintAssist.Hint=""
                                        materialDesign:TextFieldAssist.DecorationVisibility="Hidden"
                                        BorderThickness="0" MinWidth="100"
                                        TextChanged="SearchBox_TextChanged" Name="SearchBox"
                                        VerticalAlignment="Center" />
                                </StackPanel>
                            </materialDesign:ColorZone>
                            <Button Width="60" Content="Submit" HorizontalAlignment="Right" FontSize="14" Margin="10,20,20,0" Click="SubmitAttendance" 
                                    Command="{Binding SaveSubjectMultiDayAttendance}" CommandParameter="{Binding CListMultiDayAttendance}" />
                        </StackPanel>

                        <Border BorderThickness="1" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="4" Margin="15" Height="120"
                            CornerRadius="5,5,5,5" BorderBrush="LightGray" Background="Transparent" Panel.ZIndex="-98">
                        </Border>


                        <!--Group box for grid view-->
                        <Border BorderThickness="1" Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="4" Margin="10,15,10,5"
                            Panel.ZIndex="-99"
                            CornerRadius="5,5,5,5" BorderBrush="LightGray" Background="Transparent"/>
                        <!--<DataGrid x:Name="AttendanceList" ItemsSource="{Binding DataTableCollection}" IsReadOnly="True" RowHeight="30" 
                                  Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="4" VerticalAlignment="Top" AlternationCount="2" 
                                 Margin="20,25,20,15" ColumnHeaderHeight="65" Height="390" Width="1000" VerticalScrollBarVisibility="Auto"
                                  HorizontalScrollBarVisibility="Auto">
                        </DataGrid>-->
                        
                        <DataGrid IsReadOnly="True" RowHeight="30" CanUserAddRows="false" GridLinesVisibility="All" CanUserReorderColumns="False"  
                                  FrozenColumnCount="2" AutoGenerateColumns="False" ItemsSource="{Binding CListMultiDayAttendance}" HorizontalAlignment="Left" 
                                  x:Name="AttendanceList" Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="4" VerticalAlignment="Top" AlternationCount="2" 
                                  AlternatingRowBackground="{Binding Source={x:Static settings:Settings.Default}, Path=AlternatingBGColor, Mode=TwoWay }" 
                                  Margin="20,25,20,15" ColumnHeaderHeight="65" Height="390" VerticalScrollBarVisibility="Auto" >
                            <DataGrid.CellStyle>
                                <Style TargetType="DataGridCell">
                                    <Setter Property="Padding" Value="0,0,0,0"/>
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type DataGridCell}">
                                                <Border Padding="{TemplateBinding Padding}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" SnapsToDevicePixels="True">
                                                    <ContentPresenter SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"/>
                                                </Border>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </DataGrid.CellStyle>
                            <DataGrid.Resources>
                                <Style BasedOn="{StaticResource {x:Type DataGridColumnHeader}}" TargetType="{x:Type DataGridColumnHeader}">
                                    <Setter Property="Background"  Value="{Binding Source={x:Static settings:Settings.Default}, Path=Datagrid_header_multidayView, Mode=TwoWay }" />
                                    <Setter Property="Height" Value="65" />
                                </Style>
                                <Style BasedOn="{StaticResource {x:Type DataGridCell}}" TargetType="{x:Type DataGridCell}">
                                    <Setter Property="FontSize" Value="14" />
                                    <Setter Property="Padding" Value="0" />
                                    <Setter Property="FontWeight" Value="DemiBold" />
                                    <Setter Property="FontFamily" Value="Roboto" />
                                    <Setter Property="Focusable" Value="False" />
                                    <Setter Property="IsTabStop" Value="False" />
                                    <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                                </Style>
                                <Style TargetType="{x:Type ScrollBar}">
                                    <Setter Property="Stylus.IsPressAndHoldEnabled" Value="false"/>
                                    <Setter Property="Stylus.IsFlicksEnabled" Value="false"/>
                                    <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
                                    <Setter Property="Width" Value="10"/>
                                    <Setter Property="MinWidth" Value="10"/>
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type ScrollBar}">
                                                <Grid x:Name="Bg" SnapsToDevicePixels="true" Background="#7FBFBFBF">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="0.00001*"/>
                                                    </Grid.RowDefinitions>
                                                    <Track HorizontalAlignment="Center" Name="PART_Track" Width="{TemplateBinding Width}" Maximum="{TemplateBinding Maximum}" Minimum="{TemplateBinding Minimum}"
                                                        Value="{TemplateBinding Value}"  IsDirectionReversed="true">
                                                        <Track.DecreaseRepeatButton>
                                                            <RepeatButton Template="{StaticResource scroll_background}" Command="ScrollBar.LineUpCommand"  />
                                                        </Track.DecreaseRepeatButton>
                                                        <Track.IncreaseRepeatButton>
                                                            <RepeatButton Template="{StaticResource scroll_background}" Command="ScrollBar.LineDownCommand" />
                                                        </Track.IncreaseRepeatButton>
                                                        <Track.Thumb>
                                                            <Thumb Style="{StaticResource scroll_thumb_style}" >
                                                            </Thumb>
                                                        </Track.Thumb>
                                                    </Track>
                                                </Grid>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                    <Style.Triggers>
                                        <Trigger Property="Orientation" Value="Horizontal">
                                            <Setter Property="Width" Value="Auto"/>
                                            <Setter Property="MinWidth" Value="0"/>
                                            <Setter Property="Height" Value="10"/>
                                            <Setter Property="MinHeight" Value="10"/>
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="{x:Type ScrollBar}">
                                                        <Grid x:Name="Bg" SnapsToDevicePixels="true" Background="#7FA7A7A7">
                                                            <Grid.ColumnDefinitions>
                                                                <ColumnDefinition Width="0.00001*"/>
                                                            </Grid.ColumnDefinitions>
                                                            <Track x:Name="PART_Track" Grid.Column="1" IsEnabled="{TemplateBinding IsMouseOver}">
                                                                <Track.DecreaseRepeatButton>
                                                                    <RepeatButton Template="{StaticResource scroll_background}" Command="ScrollBar.LineUpCommand"  />
                                                                </Track.DecreaseRepeatButton>
                                                                <Track.IncreaseRepeatButton>
                                                                    <RepeatButton Template="{StaticResource scroll_background}" Command="ScrollBar.LineDownCommand" />
                                                                </Track.IncreaseRepeatButton>
                                                                <Track.Thumb>
                                                                    <Thumb Style="{DynamicResource scroll_thumb_stylecrosswise}" Height="8"/>
                                                                </Track.Thumb>
                                                            </Track>
                                                        </Grid>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </DataGrid.Resources>
                            <DataGrid.Columns>
                                <DataGridTextColumn HeaderTemplate="{StaticResource HeaderTemplate_STUDENTS}" Width="150" Binding="{Binding Path=STUDENT_FULLNAME}" >
                                    <DataGridTextColumn.CellStyle>
                                        <Style TargetType="DataGridCell">
                                            <EventSetter Event="MouseRightButtonDown" Handler="AddAlertComment" />
                                            <Setter Property="FontSize" Value="14" />
                                            <Setter Property="Padding" Value="0" />
                                            <Setter Property="FontWeight" Value="Bold" />
                                            <Setter Property="VerticalAlignment" Value="Stretch" />
                                            <Setter Property="TextBlock.TextAlignment" Value="Left" />
                                            <Setter Property="Focusable" Value="False" />
                                            <Setter Property="IsTabStop" Value="False" />
                                            <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                                        </Style>
                                    </DataGridTextColumn.CellStyle>
                                </DataGridTextColumn>

                                <DataGridTemplateColumn HeaderTemplate="{StaticResource HeaderTemplate_TOTAL}" >
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid>
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition/>
                                                    <ColumnDefinition/>
                                                </Grid.ColumnDefinitions>
                                                <Label FontSize="12" Margin="20 0 0 0" Grid.Column="0" FontWeight="Bold" Content="{Binding Path=TotalAbsences}" />
                                                <Label FontSize="12" Grid.Column="1" FontWeight="Bold" Content="{Binding Path=TotalTardiness}"/>
                                            </Grid>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                    <DataGridTemplateColumn.CellStyle>
                                        <Style TargetType="{x:Type DataGridCell}">
                                            <Setter Property="FontSize" Value="14" />
                                            <Setter Property="Padding" Value="0" />
                                            <Setter Property="FontWeight" Value="Bold" />
                                            <Setter Property="TextBlock.TextAlignment" Value="Center" />
                                            <Setter Property="HorizontalContentAlignment" Value="Center" />
                                            <Setter Property="HorizontalAlignment" Value="Stretch" />
                                            <Setter Property="VerticalContentAlignment" Value="Top" />
                                            <Setter Property="Focusable" Value="False" />
                                            <Setter Property="IsTabStop" Value="False" />
                                            <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                                        </Style>
                                    </DataGridTemplateColumn.CellStyle>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn HeaderTemplate="{StaticResource HeaderTemplate_Week1}" >
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <Grid >
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition />
                                                    <ColumnDefinition/>
                                                    <ColumnDefinition/>
                                                    <ColumnDefinition/>
                                                    <ColumnDefinition/>
                                                </Grid.ColumnDefinitions>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition/>
                                                </Grid.RowDefinitions>
                                                <Rectangle Grid.Column="0" Grid.Row="0" 
