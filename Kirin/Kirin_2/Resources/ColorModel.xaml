    <Color x:Key="SeriesOneColor">#0078DE</Color>
    <Color x:Key="SeriesTwoColor">#ff8c00</Color>
    <Color x:Key="SeriesThreeColor">#FFB900</Color>
    <Color x:Key="SeriesFourColor">#7A7574</Color>
